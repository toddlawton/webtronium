{
  "name": "webtronium",
  "description": "A sample website demonstrating the Webtronium stack",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Johann Troendle",
  "maintainers": [
    "Johann Troendle"
  ],
  "keywords": [
    "node",
    "react",
    "redux",
    "router",
    "universal",
    "isomorphic",
    "server-router",
    "webtronium"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JoTrdl/webtronium.git"
  },
  "scripts": {
    "start": "NODE_ENV=production node ./dist",
    "dev": "npm-run-all -p --print-label dev:*",
    "dev:client": "NODE_ENV=development webpack-dev-server --config ./config/webpack.config.js",
    "dev:server": "NODE_ENV=development BABEL_ENV=node babel-node ./src",
    "test": "npm-run-all test:*",
    "test:client": "BABEL_ENV=node jest --config config/jest.client.config.js",
    "test:server": "BABEL_ENV=node jest --config config/jest.server.config.js",
    "lint": "eslint ./src/** ./config/**",
    "build": "npm-run-all build:*",
    "build:clean": "rm -r -f ./dist",
    "build:client": "NODE_ENV=production webpack --config ./config/webpack.config.js",
    "build:server": "NODE_ENV=production BABEL_ENV=node babel ./src -d dist --ignore spec.js,test.js",
    "docco": "docco-plus -c ./public/docco.css -h Default -o ./public/annotated 'config/*.js' 'src/**/*.js' 'src/**/*.jsx'",
    "heroku-prebuild": "yarn install --production=false && npm run build"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "app-module-path": "^2.2.0",
    "classnames": "^2.2.5",
    "convict": "^4.0.1",
    "highlight.js": "^9.12.0",
    "history": "^4.7.2",
    "koa": "^2.4.1",
    "koa-helmet": "^3.3.0",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.3.0",
    "koa-static-cache": "^5.1.1",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "milligram-cssnext": "^1.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-duck": "^1.0.2",
    "redux-thunk": "^2.2.0",
    "request": "^2.83.0",
    "uncss": "^0.15.0",
    "xxhashjs": "^0.2.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-css-modules-transform": "^1.2.8",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "docco-plus": "^2.0.0",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "incstr": "^1.2.1",
    "jest": "^21.2.1",
    "json-loader": "^0.5.7",
    "node-hot": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss": "^6.0.14",
    "postcss-atroot": "^0.1.3",
    "postcss-color-function": "^4.0.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-custom-properties": "^6.2.0",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^11.0.0",
    "postcss-import-sync2": "^1.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-nesting": "^4.2.1",
    "react-test-renderer": "^16.1.1",
    "redux-logger": "^3.0.6",
    "style-loader": "^0.19.0",
    "styled-components": "^2.2.3",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-stats-plugin": "^0.1.5"
  },
  "engines": {
    "node": ">= 8.0"
  }
}
